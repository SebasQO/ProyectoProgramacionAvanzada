@model ProyectoProgramacionAvanzada2024.Models.Carrito

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>EliminarCarrito</title>
</head>
<body>
    <h3>Are you sure you want to delete this?</h3>
    <div>
        <h4>Carrito</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.CarritoId)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.CarritoId)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.CodigoCarrito)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.CodigoCarrito)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.CodigoUsuario)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.CodigoUsuario)
            </dd>

        </dl>


        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()
    <div class="form-actions no-color">
        <input type="submit" value="Eliminar" class="btn btn-default" /> |
        @Html.ActionLink("Cancelar", "ListaCarritos")  | @Html.ActionLink("Editar", "EditarCarrito", new { id = Model.CarritoId })
    </div>
        }
    </div>
</body>
</html>
